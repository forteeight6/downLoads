   "source": [
    "from pytube import YouTube, streams\n",
    "\n",
    "origin = \"\"\n",
    "destiny = \"./\"\n",
    "\n",
    "video = YouTube(origin)\n",
    "\n",
    "stream = video.streams.get_highest_resolution()\n",
    "\n",
    "stream.download(output_path=destiny)\n",
    "\n",
    "print(\"Download concluido com sucesso!\")"
   ]
  }
